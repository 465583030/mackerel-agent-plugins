#!/usr/bin/make -f

%:
	dh $@

package=mackerel-agent-plugins

override_dh_auto_install:
	dh_auto_install
	install -d -m 755 debian/tmp/usr/local/bin
	for i in apache2 mysql memcached nginx plack postgres redis aws-ec2-cpucredit elasticsearch aws-elb haproxy jvm linux mongodb aws-rds snmp squid varnish php-apc;do \
	    install -m755 debian/mackerel-plugin-$$i debian/tmp/usr/local/bin; \
	done

override_dh_usrlocal:


